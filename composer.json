{
    "name": "zimbra-api/upload-api",
    "type": "library",
    "description": "PHP wrapper library around the Zimbra upload service",
    "keywords": ["library", "upload client", "Zimbra upload"],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Nguyen Van Nguyen",
            "email": "nguyennv1981@gmail.com",
            "homepage": "https://github.com/nguyennv"
        }
    ],
    "require": {
        "php": ">=7.4",
        "php-http/multipart-stream-builder": "^1.0",
        "psr/http-client": "^1.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.0",
        "nyholm/psr7": "^1.0",
        "php-http/mock-client": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0 | ^8.0 | ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Zimbra\\Upload\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zimbra\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit",
            "vendor/bin/phpstan analyse"
        ]
    }
}
